{
  "version": 2,
  "functions": {
    "backend/app.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "backend/app.py"
    },
    {
      "src": "/scan",
      "dest": "backend/app.py"
    },
    {
      "src": "/history",
      "dest": "backend/app.py"
    },
    {
      "src": "/create-key",
      "dest": "backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/app.py"
    }
  ]
} 