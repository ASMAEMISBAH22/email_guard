{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "backend/app.py"
    },
    {
      "src": "/scan",
      "dest": "backend/app.py"
    },
    {
      "src": "/history",
      "dest": "backend/app.py"
    },
    {
      "src": "/create-key",
      "dest": "backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "backend/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "backend/app.py": {
      "maxDuration": 30
    }
  }
} 